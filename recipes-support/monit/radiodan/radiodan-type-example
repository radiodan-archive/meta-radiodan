# Radiodan services
check process radiodan-server with pidfile /var/run/radiodan/radiodan-server.pid
  group radiodan
  start program = "/etc/init.d/radiodan-server start"
  stop program = "/etc/init.d/radiodan-server stop"

check process radiodan-example with pidfile /var/run/radiodan/radiodan-example.pid
  group radiodan
  start program = "/etc/init.d/radiodan-example start"
  stop program = "/etc/init.d/radiodan-example stop"
